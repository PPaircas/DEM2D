#!/bin/sh
#

cd ../SRC
make
cd ../bin

meshfnm="../data/MESH_601m_Frontal"
iModel=1
fpeak=35    # Hz/赫兹
tmax=2.0    # s/秒
dt=0.001
srcx=300 srcz=100 sstrength=1e05
density=2200 vp=3500 vs=2000
nx=601 nz=601
dx=1.0 dz=1.0
snapt=20 mt=10

# run
    ./DEMEXE \
    meshfnm=$meshfnm iModel=$iModel fpeak=$fpeak \
    tmax=$tmax srcx=$srcx srcz=$srcz sstrength=$sstrength\
    density=$density vp=$vp vs=$vs \
    nx=$nx nz=$nz dx=$dx dz=$dz\
    snapt=$snapt mt=$mt

exit 0